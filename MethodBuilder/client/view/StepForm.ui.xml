<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:StackLayoutPanel height="426px" unit="PX" styleName="StandardStack" ui:field="MethodNavigationStack">
			<g:stack>
				<g:header size="41">Step Details</g:header>
				<g:Grid width="99%" height="100%" styleName="FormGrid" ui:field="FormTable">
					<g:row styleName="optionalHeaderStyle">
					    <g:customCell styleName="FieldName">
					      <g:Label text="Step Name:" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:TextBox ui:field="StepName" text="" width="300px" />
					    </g:customCell>
					    <g:customCell styleName="FieldError">
					    <g:Label ui:field="completionLabel1" />
					      
					    </g:customCell>
			  		</g:row>
			  		<g:row styleName="optionalHeaderStyle">
					    <g:customCell styleName="FieldName">
					      <g:Label text="Description:" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:TextArea ui:field="Description" width="300px" />
					    </g:customCell>
					    <g:customCell styleName="FieldError">
					      <g:Label ui:field="completionLabel2" />
					    </g:customCell>
			  		</g:row>
			  		<g:row styleName="optionalHeaderStyle">
					    <g:customCell styleName="FieldName">
					      <g:Label text="Creates a Study?" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:CheckBox ui:field="hasStudy" />
					    </g:customCell>
					    <g:cell></g:cell>
			  		</g:row>
					<g:row>
					    <g:customCell styleName="FieldName">
					      <g:Label text="Study Type:" ui:field="StudyTypeLabel" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:ListBox ui:field="StudyType" width="300px" >					      		
					      </g:ListBox>
					    </g:customCell>
					    <g:customCell styleName="FieldError">
					      <g:Label ui:field="completionLabel4" />
					    </g:customCell>
			  		</g:row>
			  		<g:row>
					    <g:customCell styleName="FieldName">
					      <g:Label text="DICOM Modality:" ui:field="ModalityLabel" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:ListBox ui:field="Modality" width="300px" >					      		
					      </g:ListBox>
					    </g:customCell>
					    <g:customCell styleName="FieldError">
					      <g:Label ui:field="completionLabel5" />
					    </g:customCell>
			  		</g:row>
			  		<g:row>
			  			<g:cell></g:cell>
			  			<g:customCell styleName="FieldName">
					       <g:FlexTable ui:field="MetaDatatable" styleName="MetaDataFormList" width="100%" />
					    </g:customCell>
					    <g:cell></g:cell>
					 </g:row>
			  		<g:row>
			  			<g:cell></g:cell>
			  			<g:customCell styleName="ButtonCell">
			  				<g:HorizontalPanel>
			  					<g:Button ui:field="buttonPrev" text="Previous" /><g:Button ui:field="buttonSubmit" text="Add Another Step?" />
			  				</g:HorizontalPanel>	
			  			</g:customCell>
			  					
			  			<g:cell></g:cell>
			  		</g:row>
				</g:Grid>
				     
			</g:stack>
		</g:StackLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 