<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
	<g:Grid styleName="StepHeader">
		<g:row>
			<g:customCell styleName="StepHeaderLabel">
				<g:Label ui:field="PanelTitle" />	
			</g:customCell>
			<g:customCell styleName="DeletingHeader">
				<g:PushButton ui:field="btnDelete" styleName="RemoveButton" />
			</g:customCell>
			<g:customCell styleName="EditingHeader">
				<g:PushButton ui:field="btnEdit" />	
			</g:customCell>
		</g:row>
		
		
		
	</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 