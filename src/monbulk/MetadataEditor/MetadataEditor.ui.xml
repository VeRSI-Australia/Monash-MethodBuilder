<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:w="urn:import:monbulk.shared.widgets"
	xmlns:m="urn:import:monbulk.MetadataEditor">

	<ui:style>
		.OuterContainer
		{
			margin: 5px;
		}
	</ui:style>

	<g:DockLayoutPanel unit="EM" width="100%" height="100%" styleName="{style.OuterContainer}">
		<g:west size="19">
			<m:MetadataList ui:field="m_metadataList" showRefresh="true" showNew="true" showRemove="true" showClone="true" showFromTemplate="true" />
		</g:west>
		<g:center>
			<m:MetadataProperties ui:field="m_metadataProperties" readOnly="false" />
		</g:center>
	</g:DockLayoutPanel>
</ui:UiBinder>
