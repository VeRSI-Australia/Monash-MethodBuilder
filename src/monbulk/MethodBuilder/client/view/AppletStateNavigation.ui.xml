<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:Grid styleName="AppletStateButtons">
			<g:row >
				<g:customCell>
					<g:PushButton ui:field="btnSave" styleName="btnDefault" text="SAVE"/>
				</g:customCell>
				<g:customCell>
					<g:PushButton ui:field="btnPublish" styleName="btnDefault" text="PREVIEW"/>
				</g:customCell>
				<g:customCell>
					<g:PushButton ui:field="btnCancel" styleName="btnDefault" text="CANCEL"/>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 