<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
		<g:Grid styleName="DraggableChild" ui:field="grdDragableChild">
			<g:row>
				<g:customCell styleName="LabelCell">
					<g:Label ui:field="Name" styleName="mdItemLabel" text="Test"/>
				</g:customCell>
			
				<g:customCell styleName="isMandatoryText">
				<g:Label styleName="mdItemLabel" text="Mandatory?"/>
				</g:customCell>
				<g:customCell styleName="isMandatory">
					<g:CheckBox ui:field="_cbIsMandatory" styleName="checkBox" />
						
				</g:customCell>
				<g:customCell styleName="isPublicText">
					<g:Label styleName="mdItemLabel" text="Public?"/>
				</g:customCell>
				<g:customCell styleName="isPublicCB">
					<g:CheckBox ui:field="_cbIsPublic" styleName="checkBox"/>
				</g:customCell>
					<g:customCell>
					<g:PushButton ui:field="Add" styleName="btnAdd-Blue"></g:PushButton>
				</g:customCell>
				<g:customCell>
					<g:PushButton ui:field="_btnExpand" styleName="btnExpand-Blue"></g:PushButton>
				</g:customCell>
			</g:row>
		</g:Grid>
	</g:HTMLPanel>
</ui:UiBinder> 