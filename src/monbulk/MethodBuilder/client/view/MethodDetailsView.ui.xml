<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
		.Scroll
		{
			
			border-right:Solid 2px #999;
		}
	</ui:style>
	<g:HTMLPanel>
	<g:DockLayoutPanel unit="PX" styleName="BodyPanel" height="570px" ui:field="LayoutPanel">
							<g:north size="40" unit="PX">
								<g:Grid width="100%" height="40px;">
									<g:row>
										<g:customCell styleName="MethodComponents" >
											<g:Label>Method Components</g:Label>	
										</g:customCell>
										<g:customCell styleName="FormTitleDefault" >
											<g:Label>Form</g:Label>	
										</g:customCell>
									</g:row>
									
								</g:Grid>
							</g:north>	
			 	 			<g:south size="530" unit="px" styleName="{style.Scroll}">
			 	 			<g:DockLayoutPanel unit="PX"  height="530px">
			 	 				<g:west size="287" unit="px" >
				 	 			<g:ScrollPanel width="287px" height="530px" styleName="{style.Scroll}" >
					 	 			<g:StackLayoutPanel width="274px" height="531px" unit="PX" styleName="StandardStack" ui:field="MethodNavigationStack">
										<g:stack>
											<g:header size="41" ui:field="MethodTitle">Method Details</g:header>
											<g:HTML ui:field="MethodDetailsSummary" />
										</g:stack>
										<g:stack>
											<g:header size="41">Subject Properties</g:header>
											<g:HTML ui:field="SubjectPropertiesSummary" />
										</g:stack>
									
								</g:StackLayoutPanel>
								</g:ScrollPanel>
							
								</g:west>
								
								<g:west unit="px" size="510">							
									<g:VerticalPanel>
										<g:ScrollPanel width="510px" height="480px" styleName="{style.Scroll}" >
												<g:HTMLPanel ui:field="FormDetails" />
											
										</g:ScrollPanel>
										<g:HorizontalPanel width="513px" styleName="FormNav">
											<g:PushButton ui:field="btnNext" styleName="btnDefault" text="Next"/>
										</g:HorizontalPanel>
									</g:VerticalPanel>
									</g:west>
								</g:DockLayoutPanel>
							</g:south>
						</g:DockLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 