<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.MethodComponents
		{
			width:287px;
			background-color:Orange;
			height:40px;
		}
		
		.FormTitle
		{
			width:528px;
			text-align:left;
			background-color:Blue;
			height:40px;
		}
		.Scroll
		{
			
			border-right:Solid 2px #999;
		}
	</ui:style>
	<g:HTMLPanel>
	<g:DockLayoutPanel unit="PX" styleName="BodyPanel" height="570px" ui:field="LayoutPanel">
							<g:north size="40" unit="PX">
								<g:Grid width="815px" height="40px;">
									<g:row>
										<g:customCell styleName="{style.MethodComponents}" >
											<g:Label>Method Components</g:Label>	
										</g:customCell>
										<g:customCell styleName="{style.FormTitle}" >
											<g:Label>Form</g:Label>	
										</g:customCell>
									</g:row>
									
								</g:Grid>
							</g:north>	
			 	 			<g:west size="287" unit="px" styleName="{style.Scroll}">
			 	 			<g:ScrollPanel width="287px" height="530px" styleName="{style.Scroll}" >
			 	 				
			 	 			
				 	 			<g:StackLayoutPanel width="274px" height="531px" unit="PX" styleName="StandardStack" ui:field="MethodNavigationStack">
									<g:stack>
										<g:header size="41" ui:field="MethodTitle">Method Details</g:header>
										<g:HTML ui:field="MethodDetailsSummary" />
									</g:stack>
									<g:stack>
										<g:header size="41">Subject Properties</g:header>
										<g:HTML ui:field="SubjectPropertiesSummary" />
									</g:stack>
									
								</g:StackLayoutPanel>
								</g:ScrollPanel>
							</g:west>
							
						</g:DockLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 