<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel>
	
	<g:StackLayoutPanel height="426px" unit="PX" styleName="StandardStack" ui:field="MethodNavigationStack">
			<g:stack>
				<g:header size="41">Method Form</g:header>
				<g:Grid width="99%" height="100%" styleName="FormGrid">
					<g:row styleName="optionalHeaderStyle">
					    <g:customCell styleName="FieldName">
					      <g:Label text="Method Name:" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:TextBox ui:field="titleBox" width="300px" />
					    </g:customCell>
					    <g:customCell styleName="FieldError">
					      <g:Label ui:field="completionLabel1" />
					    </g:customCell>
			  		</g:row>
			  		<g:row styleName="optionalHeaderStyle">
					    <g:customCell styleName="FieldName">
					      <g:Label text="Author:" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:Label ui:field="Author" text="Test User" />
					    </g:customCell>
					    <g:customCell styleName="FieldError">
					      <g:Label ui:field="completionLabel2" />
					    </g:customCell>
			  		</g:row>
			  		<g:row styleName="optionalHeaderStyle">
					    <g:customCell styleName="FieldName">
					      <g:Label text="Description:" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:TextArea ui:field="Description" width="300px" />
					    </g:customCell>
					    <g:customCell styleName="FieldError">
					      <g:Label ui:field="completionLabel3" />
					    </g:customCell>
			  		</g:row>
			  		<g:row styleName="optionalHeaderStyle">
					    <g:customCell styleName="FieldName">
					      <g:Label text="Keywords:" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:TextBox ui:field="keywordsBox" width="300px" />
					    </g:customCell>
					    <g:customCell styleName="FieldError">
					      <g:Label ui:field="completionLabel4" />
					    </g:customCell>
			  		</g:row>
			  		<g:row styleName="optionalHeaderStyle">
					    <g:customCell styleName="FieldName">
					      <g:Label text="Date Created:" />
					    </g:customCell>
					    <g:customCell styleName="FieldValue">
					      <g:Label ui:field="dateBox" text="15/2/2012" />
					    </g:customCell>
					    <g:customCell styleName="FieldError">
					      <g:Label ui:field="completionLabel5" />
					    </g:customCell>
			  		</g:row>			  		
			  		<g:row>
			  			<g:cell></g:cell>
			  			<g:customCell styleName="ButtonCell"><g:Button ui:field="buttonSubmit" text="Next" /></g:customCell>
			  			<g:cell></g:cell>
			  		</g:row>
				</g:Grid>
				     
			</g:stack>
		</g:StackLayoutPanel>
	</g:HTMLPanel>
</ui:UiBinder> 