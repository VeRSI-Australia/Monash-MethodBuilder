<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
		.important {
			font-weight: bold;
		}
		.menuMethodName
		{
			font-size:12px;
		}
		.menuStack
		{
	

		}
		.menuStack td
		{
			font-size:12px;
	
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel height="570px" width="210px">
		<g:ScrollPanel height="515px" width="210px">
		<!-- Have to be a list of widgets -->
			<g:FlexTable height="495px" width="210px" addStyleNames="{style.menuStack}" ui:field="_MenuStack" />
			
		</g:ScrollPanel>
		<g:PushButton ui:field="_Newbutton" text="New Method"></g:PushButton>
		</g:VerticalPanel>
		<g:MenuBar ui:field="_MethodList" vertical="true"></g:MenuBar>
	</g:HTMLPanel>
</ui:UiBinder> 
