<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HTMLPanel>
		<g:VerticalPanel>
			<g:ScrollPanel height="420px">
				<!-- Have to be a list of widgets -->
				<g:VerticalPanel>
					<g:StackLayoutPanel unit="PX" height="100%" width="100%" styleName="menuStack" ui:field="_MenuStack" />
					<g:MenuBar ui:field="_MethodList" vertical="true"></g:MenuBar>
				</g:VerticalPanel>
			</g:ScrollPanel>
			<g:PushButton ui:field="_Newbutton" text="New Method"></g:PushButton>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 