<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.important {
			font-weight: bold;
		}
		.menuMethodName
		{
			font-size:12px;
		}
	</ui:style>
	<g:HorizontalPanel ui:field="_hlContainer">
		<g:Label ui:field="lblMenuMethodName" addStyleNames='{style.menuMethodName}'>Test</g:Label>
		<g:Image></g:Image>
		<g:PushButton ui:field="btnMenuEditMethod" styleName="menuEditMethod" text="E" />
		<g:PushButton ui:field="btnMenuCloneMethod" styleName="menuCloneMethod" text="C" />
		<g:PushButton ui:field="btnMenuViewMethod" styleName="menuViewMethod" text="V" />
		<g:PushButton ui:field="btnMenuDeleteMethod" styleName="menuDeleteMethod" text="V" />
		</g:HorizontalPanel>
</ui:UiBinder> 