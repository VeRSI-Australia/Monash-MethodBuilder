<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel styleName="Container">
		
			<div class="TopNav" width="100%">
			     <g:HorizontalPanel width="100%">
					<g:MenuBar width="100%" styleName="divTopMenu">
						<g:MenuItem text="" styleName="Versi"/>
						<g:MenuItemSeparator/>
						<g:MenuItem text="DaRIS" styleName="AppTitle"/>
						<g:MenuItemSeparator/>
						<g:MenuItem text="File"/>
						<g:MenuItemSeparator/>
						<g:MenuItem text="Edit"/>
						<g:MenuItemSeparator/>
						<g:MenuItem text="View"/>
						<g:MenuItemSeparator/>
						<g:MenuItem text="Window"/>
						<g:MenuItemSeparator/>
						
					</g:MenuBar>
					<g:cell align="right">
					<g:PushButton text="" styleName="Search" ></g:PushButton>
					</g:cell>
				</g:HorizontalPanel>
			</div>
			<div class="Desktop">
				<g:HorizontalPanel>
				<g:Grid>
					<g:row>
						<g:customCell>
							<g:PushButton text="" ui:field="btnMethodApp" styleName="MethodIcon" ></g:PushButton>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:PushButton text="ARCITECTA" ui:field="btnArcMethodApp" styleName="MethodIcon" ></g:PushButton>
						</g:customCell>
					</g:row>
					<g:row>
						<g:customCell>
							<g:PushButton text="" ui:field="btnMetadataEditor" styleName="MethodIcon" />
						</g:customCell>
					</g:row>
				</g:Grid>
				<g:HTMLPanel width="1200px" height="800px" ui:field="ArcContainer"></g:HTMLPanel>
				</g:HorizontalPanel>
			</div>
			<div class="ribbon">
				<table cellpadding="0" cellspacing="0" style="width:100%;height:100%;">
					<tr>
						<td style="width:130px;vertical-align:bottom;padding-left:10px;" valign="bottom">
							<g:PushButton text="" styleName="ribbonHome" ></g:PushButton>
						</td>
						<td valign="bottom">
						 <g:TabLayoutPanel ui:field="tabPanel" barUnit="PX" barHeight="40" width="250px" height="84px" styleName="ribbon-TabLayoutPanel">
					        <g:tab styleName="ribbon-TabLayoutPanelTab">
					            <g:header>
					                Method Builder
					            </g:header>
					            <g:HTML><div class="tab1">Running</div></g:HTML>
					        </g:tab>
					        
					        
					    </g:TabLayoutPanel>
						</td>
					</tr>
					</table>			
			</div>
	</g:HTMLPanel>
</ui:UiBinder> 