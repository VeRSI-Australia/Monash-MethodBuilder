<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		
	</ui:style>
	<g:HTMLPanel styleName="WindowCaption">
		<table style="width:100%;padding:0px;"> 
		 	<tr>
		 		<td style="min-width:40%"><g:InlineLabel ui:field="m_title" styleName="AppletTitle"/></td>
		 		<td style="horizontal-align:right;vertical-align:bottom;padding-top:7px;float:right;"><g:PushButton ui:field="m_close" text="" styleName="WindowClose" /></td>
		 		<td style="horizontal-align:right;vertical-align:bottom;padding-top:7px;float:right;"><g:PushButton text="" styleName="WindowExpand" ui:field="m_maximise"/></td>
		 	</tr>
		 </table>
	</g:HTMLPanel>
</ui:UiBinder> 